blockdiag {
    static [label = "static file"];
    python [label = "Python"];
    dots [shape = "dots"];
    gui [label = "GUI"];

    html [label = "HTML"];
    vega [label = "vega spec (JSON)"];

    chart [label = "rendered chart"];

    static, python, dots, gui -> vega;
    dots -> vega [style = "none"];
    vega, html -> chart;

    group { static; python; dots; gui; color = none; }
    group { vega; html; color = none; }
}

